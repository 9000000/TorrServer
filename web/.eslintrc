{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "import/no-anonymous-default-export": 0,
    "no-plusplus": 0,
    "no-nested-ternary": 0,
    "no-shadow": 0,
    "no-unused-vars": [
      "warn",
      {
        "vars": "local",
        "args": "after-used",
        "ignoreRestSiblings": true
      }
    ]
  }
}